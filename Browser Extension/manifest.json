{
  "manifest_version": 2,

  "name": "Spoiler Watch",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Spoiler Watch!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}